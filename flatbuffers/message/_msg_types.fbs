namespace fbs.msg;

table FileChunk{
  file_name:string;
  seq:int;
  data:[byte];
}
table StringContent {
  data:string;
}

union MsgContent {
  StringContent,
  FileChunk
}

table MsgEntity{
  sender:string;
  content:MsgContent;
}

enum ChannelType:byte {
  PRIVATE = 0,
  GROUP = 1
}

table ChannelEntity {
  type:ChannelType;
  channel_id:string;
  display_name:string;
}
